{
  "name": "devfactory/media",
  "description": "Laravel package for saving uploaded files and linking them with models",
  "license": "MIT",
  "authors": [
    {
      "name": "DevFactory Sarl",
      "email": "developer@devfactory.ch"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
