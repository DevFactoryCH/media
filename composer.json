{
  "name": "devfactory/media",
  "description": "Laravel package for saving uploaded files and linking them with models",
  "license": "MIT",
  "authors": [
    {
      "name": "DevFactory Sarl",
      "email": "developer@devfactory.ch"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "~5.0",
    "that0n3guy/transliteration": "2.0.x-dev"
  },
  "autoload": {
    "classmap": [
      "src/migrations"
    ],
    "psr-4": {
      "Devfactory\\Media\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "../transliteration"
    }
  ],
  "minimum-stability": "stable"
}
